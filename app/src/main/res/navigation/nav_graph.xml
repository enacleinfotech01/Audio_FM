<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.my.audio_video_fm.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >

        <action
            android:id="@+id/action_homeFragment_to_videoFragment"
            app:destination="@id/videoFragment" />
        <action
            android:id="@+id/action_homeFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
        <action
            android:id="@+id/action_homeFragment_to_mySpaceFragment"
            app:destination="@id/mySpaceFragment" />
        <action
            android:id="@+id/action_homeFragment_to_premimumFragment"
            app:destination="@id/premimumFragment" />

    </fragment>
    <fragment
        android:id="@+id/videoFragment"
        android:name="com.my.audio_video_fm.fragment.VideoFragment"
        android:label="fragment_video"
        tools:layout="@layout/fragment_video" />

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.my.audio_video_fm.fragment.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" />
    <fragment
        android:id="@+id/mySpaceFragment"
        android:name="com.my.audio_video_fm.fragment.MySpaceFragment"
        android:label="fragment_my_space"
        tools:layout="@layout/fragment_my_space" />
    <fragment
        android:id="@+id/premimumFragment"
        android:name="com.my.audio_video_fm.fragment.PremimumFragment"
        android:label="fragment_premimum"
        tools:layout="@layout/fragment_premimum" >

    </fragment>

</navigation>